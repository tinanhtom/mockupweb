@model IEnumerable<WebQuanAoNamNu1.Model.TinTuc>
@{
    ViewBag.Title = "Index";
}
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table table-bordered table-hover">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>

        <th>
            Ngày tạo
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Image)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    
    <td>
        @Html.DisplayFor(modelItem => item.Title)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Description)
    </td>


    <td>
        @Html.DisplayFor(modelItem => item.CreatedDate)
    </td>
    <td>
        <img src="@item.Image" width="150" height="150" />
    </td>
    <td>
        @*@Html.ActionLink("Edit", "Edit", new { id = item.NewID }) |*@
        @Html.ActionLink("Details", "Details", new { id = item.ID_TT }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.ID_TT })
    </td>
</tr>
    }

</table>


